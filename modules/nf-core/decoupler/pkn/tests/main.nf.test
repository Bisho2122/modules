nextflow_process {

    name "Test Process DECOUPLER_PKN"
    script "../main.nf"
    process "DECOUPLER_PKN"

    tag "modules"
    tag "modules_nfcore"
    tag "decoupler"
    tag "decoupler/pkn"

    test("decouplerPKN_test") {

        when {
            process {
                """
                input[0] = [
                    [ id:'test' ],
                    'progeny',
                    'human'
                    ]
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
            )
        }

    }

    test("decouplerPKN_test - stub") {
        options("--stub")

        when {
            process {
                """
                input[0] = [
                    [ id:'test' ],
                    'progeny',
                    'human'
                    ]
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
            )
        }

    }

}
